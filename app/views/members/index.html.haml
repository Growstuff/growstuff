= content_for :title, "#{Growstuff::Application.config.site_name} members"

%p
  = "Displaying #{@members.length} members"

%ul.thumbnails
  - @members.each do |m|
    %li.span2
      .thumbnail(style="height: 190px")
        = render :partial => "members/avatar", :locals => { :member => m }
        = link_to m.to_s, m
