- cache cache_key_for(Member) do
  .hidden-xs
    - members = Member.interesting.limit(4)
    - if members.present?
      %h2.text-center= t('.title')
      .index-cards
        = render partial: 'members/member', collection: members
    %p.text-right
      = link_to "#{t('.view_all')} Â»", members_path, class: 'btn btn-block'
